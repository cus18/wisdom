<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wisdom.wechat.mapper.WeChatAttentionMapper">

    <insert id="insertWeChatAttention">
      INSERT INTO `wechat_attention`
      (`openid`, `marketer`, `status`, `nickname`)
      VALUES
      (#{openid}, #{marketer},#{status}, #{nickname})
    </insert>


</mapper>